{
    "name": "gtk-d",
    "targetType": "none",
    "description": "GtkD is the Digital Mars D programing language OO wrapper for Gtk+.",
    "homepage": "https://github.com/gtkd-developers/GtkD",
    "license": "LGPL license with additional exceptions",
    "authors": [
        "Frank Benoit",
        "Jake Day",
        "Jonas Kivi",
        "Alan Knowles",
        "Antonio Monteiro",
        "SebastiÃ¡n E. Peyrott",
        "John Reimer",
        "Mike Wey",
        "hauptmech"
    ],
    "dependencies": {
        "gtk-d:gtkd": "~>1.7.0",
        "gtk-d:gtkdgl": "~>1.7.0",
        "gtk-d:sv": "~>1.7.0",
        "gtk-d:gstreamer": "~>1.7.0"
    },

    "subPackages" : [
        {
            "name": "gtkd",
            "targetType": "library",
            "targetName": "gtkd-1",
            "sourcePaths": ["src"],
            "libs-posix": ["dl"],
            "importPaths": ["src"],
        },
        {
            "name": "gtkdgl",
            "targetType": "library",
            "targetName": "gtkdgl-1",
            "sourcePaths": ["srcgl"],
            "libs-posix": ["dl"],
            "importPaths": ["srcgl"],
            "dependencies": {"gtk-d:gtkd":"~>1.7.0"}
        },
        {
            "name": "sv",
            "targetType": "library",
            "targetName": "gtkdsv-1",
            "sourcePaths": ["srcsv"],
            "libs-posix": ["dl"],
            "importPaths": ["srcsv"],
            "dependencies": {"gtk-d:gtkd":"~>1.7.0"}
        },
        {
            "name": "gstreamer",
            "targetType": "library",
            "targetName": "gstreamerd-1",
            "sourcePaths": ["srcgstreamer"],
            "libs-posix": ["dl"],
            "importPaths": ["srcgstreamer"],
            "dependencies": {"gtk-d:gtkd":"~>1.7.0"}
        },
    ]
}
