{
    "name": "gtk-d",
    "targetType": "none",
    "description": "GtkD is the Digital Mars D programing language OO wrapper for Gtk+.",
    "homepage": "https://github.com/gtkd-developers/GtkD",
    "license": "LGPL license with additional exceptions",
    "authors": [
        "Frank Benoit",
        "Jake Day",
        "Jonas Kivi",
        "Alan Knowles",
        "Antonio Monteiro",
        "Sebasti√°n E. Peyrott",
        "John Reimer",
        "Mike Wey",
        "hauptmech"
    ],
    "dependencies": {
        "gtk-d:gtkd": "~master",
        "gtk-d:gtkdgl": "~master",
        "gtk-d:sv": "~master",
        "gtk-d:gstreamer": "~master",
        "gtk-d:vte": "~master"
    },

    "subPackages" : [
        {
            "name": "gtkd",
            "targetType": "library",
			"targetName": "gtkd-2",
            "sourcePaths": ["src"],
            "libs-posix": ["dl"]
        },
        {
            "name": "gtkdgl",
            "targetType": "library",
            "targetName": "gtkdgl-2",
            "sourcePaths": ["srcgl"],
            "libs-posix": ["dl"],
            "dependencies": {
		        "gtk-d:gtkd": "~master"
            }
        },
        {
            "name": "sv",
            "targetType": "library",
			"targetName": "gtkdsv-2",
            "sourcePaths": ["srcsv"],
            "libs-posix": ["dl"],
            "dependencies": {
		        "gtk-d:gtkd": "~master"
            }
        },
        {
            "name": "gstreamer",
            "targetType": "library",
			"targetName": "gstreamerd-2",
            "sourcePaths": ["srcgstreamer"],
            "libs-posix": ["dl"],
            "dependencies": {
		        "gtk-d:gtkd": "~master"
            }
        },
        {
            "name": "vte",
            "targetType": "library",
			"targetName": "vted-2",
            "sourcePaths": ["srcvte"],
            "libs-posix": ["dl"],
            "dependencies": {
		        "gtk-d:gtkd": "~master"
            }
        }
    ]
}
