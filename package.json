    "license": "LGPL license with additional exceptions",
    "authors": [
        "Frank Benoit",
        "Jake Day",
        "Jonas Kivi",
        "Alan Knowles",
        "Antonio Monteiro",
        "Sebasti√°n E. Peyrott",
